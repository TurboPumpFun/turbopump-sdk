{
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm run --r --filter \"./packages/**\" build",
    "check": "biome check --write",
    "check:types": "pnpm run --r --parallel check:types && tsc --noEmit",
    "clean": "pnpm run --r --parallel clean && rm -rf packages/**/*.json.tmp",
    "dev": "tsup --watch",
    "docs": "typedoc",
    "example:create": "dotenv -e .env tsx --tsconfig tsconfig.examples.json examples/createToken.ts",
    "format": "biome format --write",
    "preinstall": "pnpx only-allow pnpm",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "version:update": "bun scripts/updateVersion.ts",
    "version:update:viem": "bun scripts/updateViemVersion.ts"
  },
  "packageManager": "pnpm@9.11.0",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "^2.27.8",
    "@types/bun": "^1.1.10",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "bun": "^1.1.29",
    "husky": "^9.0.0",
    "sherif": "^1.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.5.4",
    "vitest": "^2.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "22.x"
  }
}
